{
  "name": "StaticS3CMS",
  "version": "1.0.0",
  "description": "Static S3-based CMS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-prod": "parcel build src/index.html --no-source-maps --out-dir build --public-url ./",
    "build": "parcel build src/index.html --out-dir build --public-url ./",
    "watch": "parcel watch src/index.html --out-dir build --public-url ./",
    "host": "parcel serve src/index.html --out-dir build --public-url ./",
    "help": "parcel help"
  },
  "repository": "http://github.com/dcrn/static-s3-cms",
  "author": "dcrn",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.430.0",
    "parcel-bundler": "^1.12.3",
    "svelte": "^2.16.1"
  },
  "devDependencies": {
    "parcel-plugin-svelte": "^2.0.0"
  }
}
